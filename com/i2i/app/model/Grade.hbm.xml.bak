<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.i2i.app.model.Grade" table="grades">
    <id name="gradeId" column="grade_id">
      <generator class="identity"/>
    </id>
    <property name="standard" column="standard" not-null="true"/>
    <property name="section" column="section" not-null="true"/>
    <set name="students" inverse="true" lazy="false">
      <key column="grade_id"/>
      <one-to-many class="com.i2i.app.model.Student"/>
    </set>
  </class>
</hibernate-mapping>