<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.i2i.app.model.BankAccount" table="bankaccount">
    <id name="accountId" column="account_id">
      <generator class="identity"/>
    </id>
    <property name="bankName" column="bank_name" not-null="true"/>
    <property name="branchName" column="branch_name" not-null="true"/>
    <property name="accountNumber" column="account_number" not-null="true" type="long"/>
	<property name="ifscCode" column="ifsc_code" />
    <property name="mobileNumber" column="mobile_number" length="10"/>
    <one-to-one name="student" class="com.i2i.app.model.Student"/>
  </class>
</hibernate-mapping>
